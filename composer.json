{
    "name": "rklandesverband/yii2-datatables",
    "description": "Datatables widget for Yii2 uses datatables",
    "type": "yii2-extension",
    "minimum-stability": "dev",
    "version" : "1.0.0",
    "keywords": ["yii2","extension","jquery","datatables","table","javascript","library"],
    "license": "MIT",
    "authors": [
        {
            "name": "Ian Schneider",
            "email": "ian.schneider@n.roteskreuz.at"
        }
    ],
    "require": {
        "yiisoft/yii2":"~2.0.13",
        "bower-asset/jquery":"~3",
        "bower-asset/datatables": "~1.10.15",
        "bower-asset/datatables-plugins": "~1.10.15"
    },
    "autoload": {
        "psr-4": {
            "rklandesverband\\datatables\\": "src/"
        }
    },
    "config": {
        "fxp-asset": {
            "installer-paths": {
                "bower-asset-library": "vendor/bower"
            }
        }
    }
}